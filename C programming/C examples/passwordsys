#include<stdio.h>
#include<string.h>

int main(){
    char user[20],pass[20];
    while(1){
        printf("\nEnter your username:");
        scanf("%s",&user);
        printf("Enter your password:");
        scanf("%s",&pass);

        if(strcmp(user,"admin")==0 && strcmp(pass,"password")==0)
        {
            printf("correct credentials");
            break;
        }
    }
    return 0;
}